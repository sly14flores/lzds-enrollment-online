<ifModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} -f [OR]
    RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} -d
    RewriteRule ^ - [L]
    RewriteRule ^ /index.html [L]
</ifModule>

#Protect Directory
AuthName "Authentication"
AuthType Basic
AuthUserFile /home/lzds/.htpasswd
Require valid-user